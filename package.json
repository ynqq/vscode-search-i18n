{
  "name": "search-i18n",
  "displayName": "search-i18n",
  "description": "1: 自动翻译语言包 2: 根据文本去匹配i18的key搜索对应的文件",
  "publisher": "ynqq",
  "version": "0.0.15",
  "engines": {
    "vscode": "^1.80.0"
  },
  "bugs": {
    "url": "https://github.com/ynqq/vscode-search-i18n/issues"
  },
  "homepage": "https://github.com/ynqq/vscode-search-i18n",
  "repository": {
    "type": "git",
    "url": "https://github.com/ynqq/vscode-search-i18n"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "search-i18n.entry": {
          "type": "string",
          "default": "zh.js",
          "description": "要搜索的语言包文件 相对于i18n-ally.localesPaths第一个值"
        },
        "search-i18n.incldesFile": {
          "type": "string",
          "default": "ts,tsx,vue",
          "description": "需要检索的文件"
        },
        "search-i18n.enableTransform": {
          "type": "boolean",
          "default": false,
          "description": "是否开启自动转换"
        },
        "search-i18n.transKey": {
          "type": "string",
          "default": "autoKey",
          "description": "自动转换的前置key"
        },
        "search-i18n.enableTrans": {
          "type": "boolean",
          "default": false,
          "description": "是否开启翻译功能，如果开启需要安装[trans-lang]"
        },
        "search-i18n.transFileConfig": {
          "type": "object",
          "default": {},
          "description": "开启翻译功能需要配置修改的文件地址, 目前只支持[jp, en]"
        }
      }
    },
    "keybindings": [
      {
        "command": "search-i18n.searchi18n",
        "key": "ctrl+i ctrl+8",
        "when": "editorTextFocus"
      },
      {
        "command": "search-i18n.replaceVueText",
        "key": "alt+d",
        "when": "editorTextFocus"
      },
      {
        "command": "search-i18n.replaceJsText",
        "key": "alt+x",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "search-i18n.searchi18n",
        "title": "searchi18n"
      },
      {
        "command": "search-i18n.replaceText",
        "title": "replaceText",
        "icon": "$(edit)"
      },
      {
        "command": "search-i18n.replaceVueText",
        "title": "replaceVueText"
      },
      {
        "command": "search-i18n.replaceJsText",
        "title": "replaceJsText"
      }
    ]
  },
  "extensionDependencies": [
    "ynqq.trans-lang"
  ],
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package",
    "publish": "vsce package && vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
